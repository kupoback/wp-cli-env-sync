{
    "name": "clique-studios/wp-cli-sync",
    "description": "A plugin for Forge websites, that syncs from one env to the next, copying the database and any uploads folders, and allowing the unpacking of the folders",
    "type": "wordpress-muplugin",
    "require": {
        "ext-json": "*",
	    "php": "^7.4|^8.0.0",
        "wp-cli/db-command": "^2.0",
        "wp-cli/extension-command": "^2.0",
        "wp-cli/maintenance-mode-command": "2.x-dev",
        "phpseclib/phpseclib": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "files": [ "forge-sync.php" ],
        "psr-4": {
            "ForgeSync\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nick Makris",
            "email": "nmakris@cliquestudios.com"
        }
    ],
    "minimum-stability": "dev"
}
