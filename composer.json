    "type": "wordpress-muplugin",
    "require": {
        "ext-json": "*",
        "php": ">=7.4"
    },
    "license": "MIT",
    "autoload": {
        "files": [ "env-sync.php" ],
        "psr-4": {
            "EnvSync\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nick Makris",
            "email": "nmakris@cliquestudios.com"
        }
    ],
    "minimum-stability": "dev"
}
